      "parameters": {
        "method": "POST",
        "url": "https://search.rcsb.org/rcsbsearch/v2/query",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"query\": {\n    \"type\": \"terminal\",\n    \"service\": \"full_text\",\n    \"parameters\": {\n      \"value\": \"{{ $json.query }}\"\n    }\n  },\n  \"return_type\": \"entry\",\n   \"request_options\": {\n        \"paginate\": {\n          \"start\": 0,\n          \"rows\": 5\n        }\n      }\n}",
        "optimizeResponse": true
